/*                                                                            */
/* ************************************************************************** */

#include "../libft.h"

char	*ft_strnmat(char **matrix, char *s, int n)
{
	int	ind;

	ind = -1;
	while (matrix[++ind])
		if (ft_strnstr(s, matrix[ind], n))
			return (matrix[ind]);
	return (NULL);
}

/*   Updated: 2025/06/11 09:10:40 by jlima-so         ###   ########.fr       */
/*                                                                            */
/* ************************************************************************** */

#include "../libft.h"

char	*ft_strmat(char **matrix, char *s)
{
	int	ind;

	ind = -1;
	while (matrix[++ind])
		if (ft_strstr(matrix[ind], s))
			return (matrix[ind]);
	return (NULL);
}

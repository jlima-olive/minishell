/*                                                                            */
/*                                                        :::      ::::::::   */
/*   ft_matlen.c                                        :+:      :+:    :+:   */
/*                                                    +:+ +:+         +:+     */
/*   By: jlima-so <jlima-so@student.42.fr>          +#+  +:+       +#+        */
/*                                                +#+#+#+#+#+   +#+           */
/*   Created: 2025/06/09 18:49:50 by jlima-so          #+#    #+#             */
/*   Updated: 2025/06/11 09:06:28 by jlima-so         ###   ########.fr       */
/*                                                                            */
/* ************************************************************************** */

#include "../libft.h"

int	ft_matlen(char **mat)
{
	int	ind;

	if (mat == NULL)
		return (0);
	ind = 0;
	while (mat[ind])
		ind++;
	return (ind);
}
